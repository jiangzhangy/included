@charset "utf-8";
/* CSS Document */

body,
blockquote,
dd,
dl,
figure,
form,
p,
pre,
h1,
h2,
h3,
h4,
h5,
h6 {margin:0; padding:0; border:0;}
ul,
li,
ol,
dl,
dt,
dd{ list-style:none; margin:0; padding:0;}
body,
input,
button,
select,
optgroup,
option,
textarea,
pre {font-family: Arial, Roboto, 'Droid Sans', 'Hiragino Sans GB', 'Microsoft YaHei', 'Simsun', STXihei, 'sans-serif';}
body{ font-size:12px; line-height:1.6;}
a {color:#000000;text-decoration:none;}
a:hover {color:#FF0000;}
img {vertical-align:middle;}
a img {border:0 none;}
.clearfix{
	content:'';
	clear:both;
	display:block;
	visibility: hidden;
	height:0;
	}
.none{
	display:none;
	}
.main{
	width:1200px;
	margin:0 auto;
	}
.index-head .logo{
	float:left;
	padding:15px 0;
	}
.index-head .tel{
	width:946px;
	float:left;
	text-align:right;
	font-size:20px;
	padding:12px 0;
	background:url(../images/telui_03.gif) no-repeat 790px center;
	color:#555;
	}
.index-head nav{
	width:946px;
	float:left;
	}
.index-head nav ul{
	float:right;
	}
.index-head nav ul li{
	width:auto;
	float:left;
	padding:0 15px;
	font-size:16px;
	}
.index-head nav ul li:last-child{
	padding-right:0;
	}
.index-bottom{
	width:100%;
	background:#00b9ce;
	float:left;
	padding:20px 0;
	margin-top:30px;
	}
.index-bottom img{
	float:left;
	}
.bottom-txt{
	float:left;
	color:rgba(255,255,255,0.6);
	padding:3px 0 0 20px;
	}