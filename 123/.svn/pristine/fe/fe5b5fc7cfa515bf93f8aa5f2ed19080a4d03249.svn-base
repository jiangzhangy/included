/*提示框*/
#alert_box{
    position:fixed;
    z-index:10000;
    top:-100%;
    word-break:break-all;
    left:50%;
    margin-left:-70px;
    border-radius:0.4em;
    width:120px;
    padding: 20px 10px 20px;
    background:rgba(0,0,0,.6);
    text-align:center;
    color:#FFF;
    font-size:1em;
    line-height:1.5em;
}
#alert_box.nodh{
    top:50%;
}
/*确认框*/
#confirm_box{
    position:fixed;
    top:-100%;
    word-break:break-all;
    z-index:100000;
    left:50%;
    margin-left:-110px;
