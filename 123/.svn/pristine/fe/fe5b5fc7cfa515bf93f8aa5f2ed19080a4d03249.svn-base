.editTable tbody tr td label{margin:0 10px 0 0; white-space:nowrap;}
.editTable tfoot tr th{border:1px solid #d6dde7; padding:6px 4px;}
.editTable tfoot tr td{border:1px solid #d6dde7; padding:6px 4px;}
