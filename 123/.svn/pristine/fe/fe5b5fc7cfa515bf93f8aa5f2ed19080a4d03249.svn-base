/* ---------------- sidebar ------------------*/
.sidebar .menu {
	margin-right: 0;
	margin-bottom: 0;
	float: none;
}
.sidebar .menu .nav {
	margin-bottom: 10px;
}
.sidebar .menu .nav .header a{
	border: 1px #B0B0B0 solid;
	-webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.5), inset 0 -1px 0 rgba(255, 255, 255, 1);
	-moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.5), inset 0 -1px 0 rgba(255, 255, 255, 1);
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1);
	-webkit-border-radius: 9px 9px 0 0;
  	-moz-border-radius: 9px 9px 0 0;
  	border-radius: 9px 9px 0 0;
	background: url(image/menu-bits.gif) 0 0;
	margin-bottom: 1px;
	font-weight: bold;
	color: #333;
	font-size: 1.1em;
	position: relative;
	height: 25px;
	padding-bottom: 0;
	padding-top: 5px;
}
.sidebar .menu .nav .header a [class^="icon-"] {
	margin: 3px 6px 0 0;
}
.sidebar .menu .nav .header a .nav-sub-toggle {
	position: absolute;
	display: block;
	cursor: pointer;
	width: 25px;
	height: 30px;
	right: 0;
	top: 0;
	background: url(image/menu-bits.gif) 0 -205px;
	text-align: center;
	line-height: 30px;
	overflow: hidden;
}
.sidebar .menu .nav-sub {
	border: 1px #B0B0B0 solid;
	border-top: none;
	padding-bottom: 1px;
	margin-bottom: 0;
	border-radius: 0 0 5px 5px;
}
.sidebar .menu .nav-sub li a {
	padding: 8px;
	background: #F6F6F6;
}
.sidebar .menu .nav-sub li a:hover {
	background: #FFF;
}
.sidebar .menu .nav-sub li.active a {
	padding-left: 15px;
	color: #333;
	font-weight: bold;
	background: #FFF;
}
.sidebar .menu .nav-sub a {
	border: none;
}
/* ---------- box ----------------*/
.box {
	padding: 10px;
	background: #FAFAFA;
	border: 1px #C0C0C0 solid;
	-webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.5), inset 0 -1px 0 rgba(255, 255, 255, 1);
	-moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.5), inset 0 -1px 0 rgba(255, 255, 255, 1);
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1), inset 0 1px 0 rgba(255, 255, 255, 1);
}
.box-radius {
	border: 1px #C0C0C0 solid;
	-webkit-border-radius: 10px;
  	-moz-border-radius: 10px;
	border-radius: 10px;
}
.box-headtitle .title {
	background: url(image/menu-bits.gif) 0 0;
	height: 20px;
	padding: 5px 10px;
	margin: -10px -10px 10px -10px;
	-webkit-border-radius: 9px 9px 0 0;
  	-moz-border-radius: 9px 9px 0 0;
  	border-radius: 9px 9px 0 0;
	border-bottom: 1px #B0B0B0 solid;
	font-weight: 700;
	font-size:15px;
}
