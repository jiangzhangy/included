.editTable textarea{border:1px solid #b4d0e5; width:60%; height:60px; padding:2px; font-size:13px;}
.editTable button{font-size:16px; padding:0 20px;}
/* swfupload 上传进度长 */
.fieldset{font-size:12px;margin:0 5px;}
.progressWrapper {overflow: hidden;} /* 进度单元 */
.progressContainer {margin:1px 0; padding:10px; background:#d6dde7; overflow: hidden;}
.message {	margin: 1em 0;	padding: 10px 20px;	border: solid 1px #FFDD99;	background-color: #FFFFCC;	overflow: hidden;}/* Message */
.progressName{float:left; font-weight:bold; height:20px;  white-space: nowrap; overflow: hidden;} /* 文件名 */
.progressBarStatus{float:left; color:#0487bb; margin:0 0 0 10px; white-space: nowrap;}/* 上传进度条：提示文字 */
.progressBarInProgress, .progressBarComplete, .progressBarError{clear:both; font-size: 0; 	width: 0%; 	height: 2px;background-color: blue; margin-top: 2px;} /* 上传进度条：进行中 */
.progressBarComplete {	width: 100%;	background-color: green;	visibility: hidden;} /* 上传进度条：完成 */
.progressBarError {width: 100%; background-color: red;visibility: hidden;} /* 上传进度条：出错 */
a.progressCancel{font-size: 0; display: block; height: 14px; width: 14px; background: #abc url(web/cancelbutton.gif) no-repeat -14px 0px; float: right;}
a.progressCancel:hover {background-position: 0 0;}

