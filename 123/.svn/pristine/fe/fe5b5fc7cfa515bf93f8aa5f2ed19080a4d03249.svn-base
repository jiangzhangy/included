.editTable thead tr td{border:1px solid #d6dde7; padding:6px 4px;}
.editTable tbody tr th{border:1px solid #d6dde7; padding:6px 4px; background:#e9eef2; text-align:right; font-weight:normal;}
.editTable tbody tr td{border:1px solid #d6dde7; padding:6px 4px; text-align:left;}
.editTable tbody tr td.labelBox label{float:left; margin:1px 10px 1px 0;}
