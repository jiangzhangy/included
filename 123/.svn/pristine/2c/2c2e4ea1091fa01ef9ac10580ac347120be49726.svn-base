body {
	margin:0 auto;
	font-size:16px;
	font-family:Verdana;
	line-height:1.5;
}
ul, dl, dd, h1, h2, h3, h4, h5, h6, form, p {
	padding:0;
	margin:0;
}
ul {
	list-style:none;
}
img {
	border:0px;
}
a {
	color:#656565;
	text-decoration:none;
}
a:hover {
	color:#44b549;
	text-decoration:none;
}
.clearfloat {
	clear:both;
	height:0;
	font-size: 1px;
	line-height: 0px;
}

